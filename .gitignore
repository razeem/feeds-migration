# Ignore binaries.
/bin

# Ignore non-custom parts of Drupal.
web/core
web/libraries
web/modules/contrib
web/profiles/contrib
web/themes/contrib
web/sites/*/files/

# Ignore settings file.
web/sites/*/settings.php
web/sites/*/settings.local.php

# Ignore paths that contain user-generated content.
web/*/files
sites/*/files
sites/*/private

# Ignore local development information
web/*/local.services.yml

# Ignore .env files as they are personal
/.env

# Ignore Composer-managed dependencies.
vendor/

# sqls and zips
/*.sql
/*.zip
/*.tar.gz
*.sql.gz

# local specific files
*.local
*.local.php

# Ignore files generated by phpstorm, netbeans and vscode
/.idea/
/nbproject/
/.vscode
.ddev/

# Generated by MacOS
.DS_Store

# Generated by Windows
Thumbs.db
